{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "root": true,
  "plugins": ["@typescript-eslint", "react"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": "error",
    "react/jsx-no-target-blank": "error",
    "no-unused-vars": "off"
  }
